"---------------VundleSettings----------------------------
set nocompatible              " be iMproved, required
filetype off                  " required





"----------------OtherVimSettingsByCsy-------------------
syntax enable
syntax on
"colorscheme molokai
"filetype indent on
"filetype on                 
"filetype plugin on
"filetype plugin indent on



set nocompatible
set ai
set nu
set number
set showmatch
set wrap
set cindent
set ruler
set tabstop=4
set softtabstop=4
set shiftwidth=4
set showcmd
set t_Co=256
set mouse=a
set encoding=utf-8
set linebreak
set backspace=indent,eol,start
set smarttab
set smartindent
set foldcolumn=4
set ignorecase
set incsearch
set hlsearch
set cmdheight=2
set paste
set fileformats=unix,dos
set ts=4
set expandtab
set autoindent

"--------------mapNewKeys------------
map <C-l> :nohl<CR>
